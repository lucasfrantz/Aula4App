<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <Button
            android:id="@+id/contactsBtn"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:onClick="onClickContacts"
            android:text="Contatos" />

        <Button
            android:id="@+id/webBtn"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:onClick="onClickWeb"
            android:text="Web" />

        <Button
            android:id="@+id/mapsBtn1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:onClick="onClickMaps1"
            android:text="Maps1" />

        <Button
            android:id="@+id/mapBtn2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:onClick="onClickMaps2"
            android:text="Maps2" />

        <Button
            android:id="@+id/mapsBtn3"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:onClick="onClickMaps3"
            android:text="Maps3" />

        <Button
            android:id="@+id/picBtn"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:onClick="onClickPicture"
            android:text="PIC ITI" />

        <TextView
            android:id="@+id/nameView"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" />

        <ImageView
            android:id="@+id/imageView"
            android:layout_width="match_parent"
            android:layout_height="313dp" />
    </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>